@{
    ViewData["Title"] = "Line Charts";
}

<div class="container mt-5">
    <h5 class="text-start mb-5">Chart Demos > Line Charts</h5>

    <div class="row mb-4">
        <div class="col-lg-3">
            <div class="card">
                <img src="~/images/chart-placeholder.jpg" class="card-img-top" alt="Basic Line Chart">
                <div class="card-body">
                    <h5 class="card-title">Basic Line Chart</h5>
                    <p class="card-text">A simple line chart to visualize data points over time.</p>
                    <a href="@Url.Action("BasicLineChart", "LineChart")" class="btn btn-primary">View Demo</a>
                </div>
            </div>
        </div>
        <div class="col-lg-3">
            <div class="card">
                <img src="~/images/chart-placeholder.jpg" class="card-img-top" alt="Dashed Line Chart">
                <div class="card-body">
                    <h5 class="card-title">Dashed Line Chart</h5>
                    <p class="card-text">A line chart with dashed lines to emphasize breaks or changes in data.</p>
                    <a href="@Url.Action("DashedLineChart", "LineChart")" class="btn btn-primary">View Demo</a>
                </div>
            </div>
        </div>
        <div class="col-lg-3">
            <div class="card">
                <img src="~/images/chart-placeholder.jpg" class="card-img-top" alt="Data Label Line Chart">
                <div class="card-body">
                    <h5 class="card-title">Data Label Line Chart</h5>
                    <p class="card-text">A line chart with data labels to show precise values for each data point.</p>
                    <a href="@Url.Action("DataLabelLineChart", "LineChart")" class="btn btn-primary">View Demo</a>
                </div>
            </div>
        </div>
        <div class="col-lg-3">
            <div class="card">
                <img src="~/images/chart-placeholder.jpg" class="card-img-top" alt="Gradient Line Chart">
                <div class="card-body">
                    <h5 class="card-title">Gradient Line Chart</h5>
                    <p class="card-text">A line chart with a gradient fill to enhance the visual appeal.</p>
                    <a href="@Url.Action("GradientLineChart", "LineChart")" class="btn btn-primary">View Demo</a>
                </div>
            </div>
        </div>
    </div>

    <div class="row mb-4">
        <div class="col-lg-3">
            <div class="card">
                <img src="~/images/chart-placeholder.jpg" class="card-img-top" alt="Line with Image Chart">
                <div class="card-body">
                    <h5 class="card-title">Line with Image Chart</h5>
                    <p class="card-text">A unique line chart with images embedded along the data points.</p>
                    <a href="@Url.Action("LineWithImageChart", "LineChart")" class="btn btn-primary">View Demo</a>
                </div>
            </div>
        </div>
        <div class="col-lg-3">
            <div class="card">
                <img src="~/images/chart-placeholder.jpg" class="card-img-top" alt="Logarithmic Line Chart">
                <div class="card-body">
                    <h5 class="card-title">Logarithmic Line Chart</h5>
                    <p class="card-text">A line chart that uses a logarithmic scale to display large range data.</p>
                    <a href="@Url.Action("LogarithmicLineChart", "LineChart")" class="btn btn-primary">View Demo</a>
                </div>
            </div>
        </div>
        <div class="col-lg-3">
            <div class="card">
                <img src="~/images/chart-placeholder.jpg" class="card-img-top" alt="Realtime Line Chart">
                <div class="card-body">
                    <h5 class="card-title">Realtime Line Chart</h5>
                    <p class="card-text">A line chart that updates in real-time with new data points.</p>
                    <a href="@Url.Action("RealtimeLineChart", "LineChart")" class="btn btn-primary">View Demo</a>
                </div>
            </div>
        </div>
        <div class="col-lg-3">
            <div class="card">
                <img src="~/images/chart-placeholder.jpg" class="card-img-top" alt="Missing Data Line Chart">
                <div class="card-body">
                    <h5 class="card-title">Missing Data Line Chart</h5>
                    <p class="card-text">A line chart that handles and displays missing data points.</p>
                    <a href="@Url.Action("MissingDataLineChart", "LineChart")" class="btn btn-primary">View Demo</a>
                </div>
            </div>
        </div>
    </div>

    <div class="row mb-4">
        <div class="col-lg-3">
            <div class="card">
                <img src="~/images/chart-placeholder.jpg" class="card-img-top" alt="Zoomable Time Series Chart">
                <div class="card-body">
                    <h5 class="card-title">Zoomable Time Series Chart</h5>
                    <p class="card-text">A time series chart with zooming and panning capabilities.</p>
                    <a href="@Url.Action("ZoomableTimeSeriesChart", "LineChart")" class="btn btn-primary">View Demo</a>
                </div>
            </div>
        </div>
                <div class="col-lg-3">
            <div class="card">
                <img src="~/images/chart-placeholder.jpg" class="card-img-top" alt="Stepline Chart">
                <div class="card-body">
                    <h5 class="card-title">Stepline Chart</h5>
                    <p class="card-text">A stepline chart to display data with sudden changes.</p>
                    <a href="@Url.Action("SteplineChart", "LineChart")" class="btn btn-primary">View Demo</a>
                </div>
            </div>
        </div>
    </div>


</div>
